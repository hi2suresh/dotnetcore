@page
@model OdeToFoodCopy.Pages.Restaurants.ListModel
@{
}
<h1>Restaurants</h1>

@Model.Message
<h2>Suresh Learning dotnet core</h2>

<table class="table">
    @foreach (var restaurant in Model.Restaurants)
    {
    <tr>
        <td>@restaurant.Name</td>
        <td>@restaurant.Location</td>
        <td>@restaurant.Cuisine</td>

    </tr>
    }
</table>